class Solution {
    public long countCompleteDayPairs(int[] hours) 
    {

        //int result=0;
        long result=0;
        int[] count = new int[24];

        for(int i=0; i<hours.length;i++)
        {
            result += count[(24-hours[i] %24)%24];
            count[hours[i] % 24]++;

        }

        return result;
        
    }
}
