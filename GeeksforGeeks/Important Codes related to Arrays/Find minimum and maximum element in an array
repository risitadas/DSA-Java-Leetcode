class Compute 
{
    static Pair getMinMax(long a[], long n)  
    {
        //Write your code here
        
        
        long max = Integer.MIN_VALUE;
        long min = Integer.MAX_VALUE;
        
        for(int i=0; i< n; i++)
        {
            
            if(min > a[i])
            {
                min = a[i];
                
            }
            
            if(max < a[i])
            {
                max = a[i];
                
            }
            
            
        }
        
        Pair p = new Pair(min, max);
        
        return p;
        
        
    }
}
