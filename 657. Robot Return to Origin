class Solution {
    public boolean judgeCircle(String moves) 
    {

        int m = 0, n = 0;

        for(int i = 0; i<moves.length(); i++)
        {

            char move = moves.charAt(i);

            if(move == 'U')
            {
                n--;


            }

            else if(move == 'D')
            {
                n++;

            }
            else if(move == 'L')
            {
                m--;

            }
            else if(move == 'R')
            {
                m++;
            }
        }

        return m == 0 && n == 0;









        
    }
}
